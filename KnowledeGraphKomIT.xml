<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema"
         xml:base="http://www.komit.com/ontology/OntoInfraGML/ontoinfragml.owl">

    <owl:Ontology rdf:about="http://www.komit.com/ontology/OntoInfraGML/ontoinfragml.owl">
    </owl:Ontology>

    <!--  
        ///////////////////////////////////////////////////////////////////////////////////////
        //
        // Annotation properties
        //
        ///////////////////////////////////////////////////////////////////////////////////////
    -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2000/01/rdf-schema#comment"/>

   <!--  
        ///////////////////////////////////////////////////////////////////////////////////////
        //
        // Classes
        //
        ///////////////////////////////////////////////////////////////////////////////////////
    -->

    <owl:Class rdf:about="#Feature">
        <rdfs:subClassOf rdf:resource="http://www.theworldavatar.com/ontology/ontocitygml/OntoCityGML.owl#AbstractCityObjectType"/>
    </owl:Class>

    <owl:Class rdf:about="#Facility">
    <rdfs:subClassOf rdf:resource="#Feature"/>
    </owl:Class>

    <owl:Class rdf:about="#FacilityPart">
        <rdfs:subClassOf rdf:resource="#Facility"/>
    </owl:Class>

    <owl:Class rdf:about="#PhysicalElement">
        <rdfs:subClassOf rdf:resource="#FacilityPart"/>
    </owl:Class>

    <owl:Class rdf:about="#Building">
        <rdfs:subClassOf rdf:resource="#FacilityPart"/>
        <rdfs:subClassOf rdf:resource="http://www.theworldavatar.com/ontology/ontocitygml/OntoCityGML.owl#AbstractBuildingType"/>
    </owl:Class>

    <owl:Class rdf:about="#Bridge">
        <rdfs:subClassOf rdf:resource="#FacilityPart"/>
    </owl:Class>
    <owl:Class rdf:about="#Drainage">
        <rdfs:subClassOf rdf:resource="#FacilityPart"/>
    </owl:Class>
    <owl:Class rdf:about="#Environmental">
        <rdfs:subClassOf rdf:resource="#FacilityPart"/>
    </owl:Class>
    <owl:Class rdf:about="#Site">
        <rdfs:subClassOf rdf:resource="#FacilityPart"/>
    </owl:Class>
    <owl:Class rdf:about="#Tunnel">
        <rdfs:subClassOf rdf:resource="#FacilityPart"/>
    </owl:Class>
    <owl:Class rdf:about="#Wastewater">
        <rdfs:subClassOf rdf:resource="#FacilityPart"/>
    </owl:Class>
    <owl:Class rdf:about="#WaterDistribution">
        <rdfs:subClassOf rdf:resource="#FacilityPart"/>
    </owl:Class>
    <owl:Class rdf:about="#OtherFacility">
        <rdfs:subClassOf rdf:resource="#FacilityPart"/>
    </owl:Class>
    <owl:Class rdf:about="#Project"/>
    <owl:Class rdf:about="#ProjectPart">
        <rdfs:subClassOf rdf:resource="#Project"/>
    </owl:Class>
    <owl:Class rdf:about="#LandFeature">
        <rdfs:subClassOf rdf:resource="#Feature"/>
    </owl:Class>
    <owl:Class rdf:about="#LandElement"/>
    <owl:Class rdf:about="#LandSurface">
        <rdfs:subClassOf rdf:resource="http://www.theworldavatar.com/ontology/ontocitygml/OntoCityGML.owl#TINReliefType"/>
    </owl:Class>
    <owl:Class rdf:about="#LandLayer"/>
    <owl:Class rdf:about="#SolidLayer">
        <rdfs:subClassOf rdf:resource="#LandLayer"/>
    </owl:Class>
    <owl:Class rdf:about="#SurfacesLayer">
        <rdfs:subClassOf rdf:resource="#LandLayer"/>
    </owl:Class>
    <owl:Class rdf:about="#LinearLayer">
        <rdfs:subClassOf rdf:resource="#LandLayer"/>
    </owl:Class>
    
    <owl:Class rdf:about="#Road">
        <rdfs:subClassOf rdf:resource="#FacilityPart"/>
    </owl:Class>
    <owl:Class rdf:about="#RoadElement">
        <rdfs:subClassOf rdf:resource="#Road"/>
    </owl:Class>
    <owl:Class rdf:about="#RoadElementSet">
        <rdfs:subClassOf rdf:resource="#RoadElement"/>
    </owl:Class>
    <owl:Class rdf:about="#StringLine">
        <rdfs:subClassOf rdf:resource="#Road"/>
    </owl:Class>
    <owl:Class rdf:about="#StringLineSet">
        <rdfs:subClassOf rdf:resource="#StringLine"/>
    </owl:Class>
    <owl:Class rdf:about="#Surface">
        <rdfs:subClassOf rdf:resource="#Road"/>
    </owl:Class>
    <owl:Class rdf:about="#SurfaceSet">
        <rdfs:subClassOf rdf:resource="#Surface"/>
    </owl:Class>
    <owl:Class rdf:about="#RoadCrossSection">
        <rdfs:subClassOf rdf:resource="#Road"/>
    </owl:Class>
    <owl:Class rdf:about="#RoadWithCrossSection"/>
    <owl:Class rdf:about="#CrossSectionSet"/>
    <owl:Class rdf:about="#CrossSectionArea"/>
    <owl:Class rdf:about="#CrossSectionComponent"/>
    <owl:Class rdf:about="#CrossSectionPoint"/>


   <!--  
        ///////////////////////////////////////////////////////////////////////////////////////
        //
        // Object Properties
        //
        ///////////////////////////////////////////////////////////////////////////////////////
    -->


    <owl:ObjectProperty rdf:about="#hasSpatialRepresentation">
        <rdfs:domain rdf:resource="#Feature"/>
        <rdfs:range rdf:resource="#SpatialRepresentation"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#isPart">
        <rdfs:domain rdf:resource="#FacilityPart"/>
        <rdfs:range rdf:resource="#Facility"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#hasElement">
        <rdfs:domain rdf:resource="#FacilityPart"/>
        <rdfs:range rdf:resource="#PhysicalElement"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#hasProjectPart">
        <rdfs:domain rdf:resource="#Project"/>
        <rdfs:range rdf:resource="#ProjectPart"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#associatedWithFacilityPart">
        <rdfs:domain rdf:resource="#ProjectPart"/>
        <rdfs:range rdf:resource="#FacilityPart"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#belongsToLayer">
        <rdfs:domain rdf:resource="#LandFeature"/>
        <rdfs:range rdf:resource="#LandLayer"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#hasCrossSectionArea">
        <rdfs:domain rdf:resource="#LinearLayer"/>
        <rdfs:domain rdf:resource="#LandElement"/>
        <rdfs:domain rdf:resource="#LandSurface"/>
        <rdfs:domain rdf:resource="#LandLayer"/>
        <rdfs:domain rdf:resource="#SolidLayer"/>
        <rdfs:range rdf:resource="#CrossSectionArea"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#hasLandCrossSection">
        <rdfs:domain rdf:resource="#LinearLayer"/>
        <rdfs:domain rdf:resource="#LandElement"/>
        <rdfs:domain rdf:resource="#LandSurface"/>
        <rdfs:domain rdf:resource="#LandLayer"/>
        <rdfs:domain rdf:resource="#SolidLayer"/>
        <rdfs:range rdf:resource="#LandCrossSection"/>
    </owl:ObjectProperty>
        <owl:ObjectProperty rdf:about="#hasCrossSection">
        <rdfs:domain rdf:resource="#RoadCrossSection"/>
        <rdfs:range rdf:resource="#RoadWithCrossSection"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#partOfCrossSectionSet">
        <rdfs:domain rdf:resource="#RoadCrossSection"/>
        <rdfs:range rdf:resource="#CrossSectionSet"/>
    </owl:ObjectProperty>

   <!--  
        ///////////////////////////////////////////////////////////////////////////////////////
        //
        // Data Properties
        //
        ///////////////////////////////////////////////////////////////////////////////////////
    -->

    <owl:DatatypeProperty rdf:about="#ID">
        <rdfs:domain rdf:resource="#Feature"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    <owl:DatatypeProperty rdf:about="#Status">
        <rdfs:domain rdf:resource="#Feature"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    <owl:DatatypeProperty rdf:about="#Percentage">
        <rdfs:domain rdf:resource="#Feature"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    </owl:DatatypeProperty>
    

</rdf:RDF>